package com.fannie.step;

import cucumber.api.java.en.And;
import cucumber.api.java.en.But;
import cucumber.api.java.en.Given;
import cucumber.api.java.en.Then;
import cucumber.api.java.en.When;

public class LoanSanctionStep {

	@And("working in govt office")
	public void working_in_govt_office() {
		System.out.println("working in govt office>>>>>");
	}

	@When("credit score is more than 500")
	public void credit_score_is_more_than_500() {
		System.out.println("credit score is more than 500>>>>>>>");
	}

	@Then("Sanction loan")
	public void Sanction_loan() {
		System.out.println("Sanction loan>>>>>");
	}
	
	
	
	

	@But("customer has to repay with in 20 years")
	public void customer_has_to_repay_with_in_20_years() {
		System.out.println("customer has to repay with in 20 years>>>>>>");
	}
	
	
	
	
	
	

	// * + ? . 
	@Given("the customer works ([A-Za-z]{1,}) time")
	public void the_customer_works_full_time(String workTime) {
		System.out.println("the customer works full time>>>>>");
	}

	
	
	

	@Given("^the customer works part time$")
	public void the_customer_works_part_time() throws Throwable {
		System.out.println("the customer works part time>>>>>>>>>>>>");
	}
	
	
	
	
	
	

	@Given("^working in private office$")
	public void working_in_private_office() throws Throwable {
		System.out.println("the customer works part time>>>>>>>>");
	}

}
